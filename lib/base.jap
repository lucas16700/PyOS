(program
    (TARGET set block)
    (DEF hover (int x int y) 
        (
            (change color (255 255 255))
        )
        (RETURN 0)
    )
    (EVENT hover set hover)
    (DEF quit (int a)
        (
            (halt)
        )
        (RETURN 0)
    )
    (halt)
    (EVENT !event.quit set quit)
)
(meta
    (version 0)
    (budleid (10 2 3))
    (name "teste")
)
